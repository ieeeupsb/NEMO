<?xml version="1.0" ?>
<sdf version="1.7">
    <model name="nemo">
        <pose>0 0 0.5 0 0 0</pose>

        <static>false</static>

        <!-- Differential Drive Plugin -->
        <plugin name="ignition::gazebo::systems::DiffDrive" filename="libignition-gazebo-diff-drive-system.so">
            <left_joint>left_wheel_joint</left_joint>
            <right_joint>right_wheel_joint</right_joint>
            <wheel_separation>2.0</wheel_separation>
            <wheel_radius>0.3</wheel_radius>
            <odom_publish_frequency>2</odom_publish_frequency>
        </plugin>

        <!-- Sensors Plugin -->
        <plugin name="ignition::gazebo::systems::Sensors" filename="libignition-gazebo-sensors-system.so">
            <render_engine>ogre2</render_engine>
        </plugin>

        <link name='chassis'>
            <inertial>
                <mass>1.14395</mass>

                <inertia>
                    <ixx>0.126164</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.416519</iyy>
                    <iyz>0</iyz>
                    <izz>0.481014</izz>
                </inertia>
            </inertial>

            <visual name='front'>
                <pose>0.5 0 0 0 0 0</pose>

                <geometry>
                    <box>
                        <size>1.0 2.0 0.8</size>
                    </box>
                </geometry>

                <!-- Orange -->
                <material>
                    <ambient>1 0.3 0.1 1</ambient>
                    <diffuse>1 0.3 0.1 1</diffuse>
                    <specular>1.0 1.0 1.0 1</specular>
                </material>
            </visual>

            <visual name='back'>
                <geometry>
                    <cylinder>
                        <radius>1.0</radius>
                        <length>0.8</length>
                    </cylinder>
                </geometry>

                <!-- Orange -->
                <material>
                    <ambient>1 0.3 0.1 1</ambient>
                    <diffuse>1 0.3 0.1 1</diffuse>
                    <specular>1.0 1.0 1.0 1</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <box>
                        <size>2.0 2.0 0.8</size>
                    </box>
                </geometry>
            </collision>
        </link>

        <!-- Left Wheel -->
        <link name='left_wheel'>
            <pose>0.5 1.1 -0.2 1.57 0 0</pose>

            <inertial>
                <mass>2</mass>
                <inertia>
                    <ixx>0.145833</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.145833</iyy>
                    <iyz>0</iyz>
                    <izz>0.125</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.3</radius>
                        <length>0.1</length>
                    </cylinder>
                </geometry>

                <material>
                    <ambient>0.2 0.2 0.2 1</ambient>
                    <diffuse>0.2 0.2 0.2 1</diffuse>
                    <specular>0.2 0.2 0.2 1</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.3</radius>
                        <length>0.1</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <joint name='left_wheel_joint' type='revolute'>
            <pose relative_to='left_wheel'/>
            <parent>chassis</parent>
            <child>left_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                </limit>
            </axis>
        </joint>

        <!-- Right Wheel -->
        <link name='right_wheel'>
            <pose>0.5 -1.1 -0.2 1.57 0 0</pose>

            <inertial>
                <mass>2</mass>
                <inertia>
                    <ixx>0.145833</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.145833</iyy>
                    <iyz>0</iyz>
                    <izz>0.125</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.3</radius>
                        <length>0.1</length>
                    </cylinder>
                </geometry>

                <material>
                    <ambient>0.2 0.2 0.2 1</ambient>
                    <diffuse>0.2 0.2 0.2 1</diffuse>
                    <specular>0.2 0.2 0.2 1</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.3</radius>
                        <length>0.1</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <joint name='right_wheel_joint' type='revolute'>
            <pose relative_to='right_wheel'/>
            <parent>chassis</parent>
            <child>right_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                </limit>
            </axis>
        </joint>

        <!-- Caster wheel -->
        <link name="caster_wheel">
            <pose>-0.8 0 -0.4 0 0 0</pose>

            <visual name='visual'>
                <geometry>
                    <sphere>
                        <radius>0.1</radius>
                    </sphere>
                </geometry>

                <material>
                    <ambient>0.2 0.2 0.2 1</ambient>
                    <diffuse>0.2 0.2 0.2 1</diffuse>
                    <specular>0.2 0.2 0.2 1</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <sphere>
                        <radius>0.1</radius>
                    </sphere>
                </geometry>
            </collision>
        </link>

        <joint name="caster_wheel_joint" type="fixed">
            <parent>chassis</parent>
            <child>caster_wheel</child>
            <axis>
                <xyz>0.24 0 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                </limit>
            </axis>
        </joint>

        <!-- RGB Camera -->
        <link name="camera_link">
            <pose>0.9 0 0.7 0 0.6 0</pose>

            <visual name='visual'>
                <geometry>
                    <box>
                        <size>0.5 0.5 0.5</size>
                    </box>
                </geometry>

                <material>
                    <ambient>1.0 1.0 1.0 1</ambient>
                    <diffuse>1.0 1.0 1.0 1</diffuse>
                    <specular>0.0 0.0 1.0 1</specular>
                </material>
            </visual>

            <sensor name="camera" type="camera">
                <camera>
                    <horizontal_fov>1.047</horizontal_fov>

                    <image>
                        <width>800</width>
                        <height>800</height>
                    </image>

                    <clip>
                        <near>0.1</near>
                        <far>100</far>
                    </clip>
                </camera>

                <pose>0 0 0 0 0 0</pose>
                <always_on>1</always_on>
                <update_rate>30</update_rate>
                <visualize>true</visualize>
                <topic>camera</topic>

                <save enabled="false">
                    <path>camera/</path>
                </save>
            </sensor>
        </link>

        <joint name="camera_joint" type="fixed">
            <parent>chassis</parent>
            <child>camera_link</child>
            <axis>
                <xyz>0 0.5 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                </limit>
            </axis>
        </joint>
    </model>
</sdf>
